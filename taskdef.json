{
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "family": "$TASK_FAMILY",
  "executionRoleArn": "$executionRoleArn",
  "containerDefinitions": [
    {
      "image": "221877401369.dkr.ecr.ap-northeast-2.amazonaws.com/httpd:ad97377",
      "name": "$CONTAINER_NAME",
      "mountPoints": [],
      "logConfiguration": {
        "options": {
          "awslogs-group": "$awslogs_group",
          "awslogs-region": "$REGION",
          "awslogs-stream-prefix": "$awslogs_stream_prefix"
        },
        "logDriver": "awslogs"
      },
      "cpu": 0,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": $CONTAINER_PORT,
          "hostPort": 80
        }
      ],
      "environment": [],
      "essential": true
    }
  ],
  "memory": "$TASK_MEMORY",
  "cpu": "$TASK_CPU"
}